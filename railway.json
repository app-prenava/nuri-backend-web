{
    "$schema": "https://railway.app/railway.schema.json",
    "build": {
      "builder": "DOCKERFILE",
      "dockerfilePath": "docker/frankenphp.Dockerfile"
    },
    "deploy": {
      "startCommand": "frankenphp run --config /etc/caddy/Caddyfile",
      "restartPolicyType": "ON_FAILURE",
      "restartPolicyMaxRetries": 10
    }
  }